import org.code.neighborhood.*;

public class NeighborhoodRunner {
  public static void main(String[] args) {

    // Creates a Painter object
    Painter silas = new Painter();

    // Moves forward three spaces
    silas.move();
    silas.move();
    silas.move();

    // Turns right by turning left three times
    silas.turnLeft();
    silas.turnLeft();
    silas.turnLeft();

    // Moves forward two spaces
    silas.move();
    silas.move();

    /* ---- ðŸ”Ž ADD YOUR CODE BELOW THIS LINE ---- */

    silas.turnLeft();
    silas.turnLeft();

    silas.move();
    silas.move();
    silas.turnLeft();

    silas.move();
    silas.move();
    silas.move();

    silas.turnLeft();
    silas.turnLeft();
    
  }
}
